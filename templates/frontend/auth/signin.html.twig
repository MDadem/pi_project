		<link rel="stylesheet" href="{{ asset('css/form-util.css') }}">
		<link rel="stylesheet" href="{{ asset('css/form-main.css') }}">
	</head>
	<body>

		<div class="limiter">
			<div class="container-login100">
				<div class="wrap-login100">
					<div class="login100-form-title" style="background-image: url({{ asset('img/bg-01.jpg') }});">
						<span class="login100-form-title-1">
							Sign In
						</span>
					</div>

					{# Display authentication errors #}
					{% if error %}
						<div class="alert alert-danger" role="alert">
							{{ error.messageKey|trans(error.messageData, 'security') }}
						</div>
					{% endif %}

					{# Display flash messages #}
					{% for message in app.flashes('success') %}
						<div class="alert alert-success" role="alert">
							{{ message }}
						</div>
					{% endfor %}

					{% for message in app.flashes('warning') %}
						<div class="alert alert-warning" role="alert">
							{{ message }}
						</div>
					{% endfor %}

					<form class="login100-form validate-form" method="POST" action="{{ path('app_home_signin') }}">
						<input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">

						<div class="wrap-input100 validate-input m-b-26" data-validate="Email is required">
							<span class="label-input100">Email</span>
							<input class="input100" type="text" name="email" placeholder="you email"  value="{{ last_username }}">
							<span class="focus-input100"></span>
						</div>

						<div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
							<span class="label-input100">Password</span>
							<input class="input100" type="password" name="password" placeholder="Enter password">
							<span class="focus-input100"></span>
						</div>

						<div class="flex-sb-m w-full p-b-30">
							<div class="contact100-form-checkbox">
								<input class="input-checkbox100" id="ckb1" type="checkbox" name="_remember_me">
								<label class="label-checkbox100" for="ckb1">
									Remember me
								</label>
							</div>
							<div>
								<a href="{{ path('app_forgot_password') }}" class="txt1">
									Forgot Password?
								</a>
								<span class="txt1">
									|
								</span>
								<a href="{{ path('app_home_signup') }}" class="txt1">
									Not Registered yet?
								</a>
							</div>
						</div>

						<div class="container-login100-form-btn">
							<button class="login100-form-btn" type="submit">
								Login
							</button>
						</div>
					</form>
				</div>
			</div>
		</div>

